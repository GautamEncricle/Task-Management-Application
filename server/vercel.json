{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "name": "task-management-application",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}